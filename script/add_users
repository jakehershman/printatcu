#!/usr/bin/env ruby
# encoding: utf-8

require File.expand_path("../../config/environment",  __FILE__)

users = [
  "Shifty Moon",
  "Waffle Shuffle",
  "Lee C. Bollinger",
  "Geetika Rudra",
  "Prince George Alexander Louis",
  "Ted Bundy",
  "Happy Belated Claire Parker"
]

$redis.multi
$redis.del("users")
users.each do |user|
  $redis.lpush("users", user)
end
$redis.exec